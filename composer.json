{
    "name": "g3n1us/laravel-model-api",
    "description": "Provides a unified api to access model data using basic naming conventions",
    "type": "package",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Sean Bethel",
            "email": "sean@americanmadeweb.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.2",
        "laravel/framework": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "G3n1us\\ModelApi\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "G3n1us\\ModelApi\\ServiceProvider"
            ],
            "aliases": {
                "Editor": "G3n1us\\ModelApi\\ServiceProvider"
            }
        }
    }
}
